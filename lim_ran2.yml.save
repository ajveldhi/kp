apiVersion: v1
kind: LimitRange

metadata:
  name: testrange
  namespace: test
spec:
  limits:
    - default:
        cpu: 200m
        memory: 500Mi
      type: Container
      defaultRequest:
        cpu: 100m
        memory: 250Mi
      min:
        cpu: 80m
        memory: 250Mi
      max:
        cpu: 700m
        
